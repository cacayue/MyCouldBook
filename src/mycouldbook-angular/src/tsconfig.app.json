{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "esnext",
    "types": ["node"],
    "paths": {
      "@abp/*": ["../node_modules/abp-ng2-module/dist/src/*"],
      "@appshared/*": ["app/shared/*"],
      "@testing": ["testing"],
      "@testing/*": ["testing/*"],
      "@env": ["environments"],
      "@env/*": ["environments/*"],
      "@shared/*": ["shared/*"],
      "@app/*": ["./app/*"],
      "@layout/*": ["./layout/*"],
      "@yoyo": ["../node_modules/yoyo-ng-module/src"],
      "@yoyo/*": ["../node_modules/yoyo-ng-module/src/*"],
    },
    "baseUrl": "./"
  },
  "exclude": ["src/test.ts", "**/*.spec.ts"]
}
